{
  "builds": [
    {
      "src": "config/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9", 
        "src": "build.sh"
    }
    }
  ],
  "routes": [
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "config/wsgi.py"
    }
  ]
}